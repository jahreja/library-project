<h2 class="page-header">All Runs</h2>

<form action="/runs" method="GET">



</form>
<div class="run-grid">
    <% runs.forEach(run => { %>
        <a href="/runs/<%= run.id %>">
            <img class="ra-image" height="350" width="350" src="<%= run.coverImagePath %>">
        </a>
    <% }) %>
</div>